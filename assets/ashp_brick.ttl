@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix occ: <https://brickschema.org/schema/Brick/extension/occupancy#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tag: <https://brickschema.org/schema/BrickTag#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix smallOff: <https://NIST.gov/example#/> .

# NIST-ASHP
# Notes:
# 	Point label final letters
# 		_M: Measured, i.e. in emulated system
# 		_C: Calculated, using measured values
# 		_S: Simulated, generated by EnergyPlus


# GRID SIGNALS
smallOff:grid_signal_type a brick:Status . # Grid signal type (0 no signal, 1 shedding request, 2 time-of-use price, 3 mod)
smallOff:grid_signal_shed a brick:Status . # Grid signal shedding request (0 - no request, 1 - request)
smallOff:grid_signal_price a brick:Status ; # Grid signal time-of-use price (USD)
	rdf:label "Currency, USD" .

# HVAC SYSTEM
smallOff:ASHP a brick:AHU ;
	brick:hasPoint smallOff:sa_temp, smallOff:sa_temp_dp, smallOff:sa_flow,
	smallOff:ref_flow, smallOff:stat, smallOff:stg, smallOff:id_power, smallOff:od_power .
	
smallOff:sa_temp a brick:Supply_Air_Temperature_Sensor ;
	brick:hasUnits unit:DEG_F .
smallOff:sa_temp_dp a brick:Dewpoint_Sensor ;
	brick:hasUnits unit:DEG_F .
smallOff:sa_flow a brick:Supply_Air_Flow_Sensor ;
	brick:hasUnits unit:FT3-PER-MIN ;
	rdf:label "at Standard Air Density (0.075 lbm/ft3)" .
smallOff:ref_flow a brick:Flow_Sensor ; # Placeholder
	brick:hasUnits unit:UNITLESS ;
	rdf:label "Refrigerant, lbm/min" .
smallOff:stat a brick:On_Off_Status .
smallOff:stg a brick:Stages_Status .
smallOff:id_power a brick:Power_Sensor ;
	brick:hasUnits unit:W .
smallOff:od_power a brick:Power_Sensor ;
	brick:hasUnits unit:W .

# SITE
smallOff:site_smallOff a brick:site ;
	brick:isLocationOf smallOff:building_smallOff ;
	brick:hasPoint smallOff:oa_temp, smallOff:oa_temp_dp, smallOff:oa_temp_sim, smallOff:oa_temp_dp_sim .
	
smallOff:oa_temp a brick:Outside_Air_Temperature_Sensor ;
	brick:hasUnits unit:DEG_F .
smallOff:oa_temp_dp a brick:Outside_Air_Dewpoint_Sensor ;
	brick:hasUnits unit:DEG_F .
smallOff:oa_temp_sim a brick:Outside_Air_Temperature_Sensor ;
	brick:hasUnits unit:DEG_F .
smallOff:oa_temp_dp_sim a brick:Outside_Air_Dewpoint_Sensor ;
	brick:hasUnits unit:DEG_F .


# ARCHITECTURAL LAYOUT
smallOff:building_smallOff a brick:Building ;
	brick:isLocationOf smallOff:Zone .

smallOff:Zone    a   brick:HVAC_Zone ;
    brick:isFedBy	smallOff:ASHP ;
	brick:Feeds	smallOff:ASHP ;
	brick:has_Room   smallOff:officeRoom .

smallOff:officeRoom    a   brick:Room ;
	brick:hasPoint smallOff:zn_temp, smallOff:zn_temp_dp, smallOff:local_pres, smallOff:zn_temp_cspt, smallOff:zn_temp_hspt,
		smallOff:zn_temp_sim, smallOff:zn_temp_dp_sim .
	
smallOff:zn_temp a brick:Zone_Air_Temperature_Sensor ;
	brick:hasUnits unit:DEG_F .
smallOff:zn_temp_dp a brick:Zone_Air_Dewpoint_Sensor ;
	brick:hasUnits unit:DEG_F .
smallOff:local_pres a brick:Pressure_Sensor ;
	brick:hasUnits unit:IN_HG .
smallOff:zn_temp_cspt a brick:Zone_Air_Cooling_Temperature_Setpoint ;
	brick:hasUnits unit:DEG_F .
smallOff:zn_temp_hspt a brick:Zone_Air_Heating_Temperature_Setpoint ;
	brick:hasUnits unit:DEG_F .
smallOff:zn_temp_sim a brick:Zone_Air_Temperature_Sensor ;
	brick:hasUnits unit:DEG_F .
smallOff:zn_temp_dp_sim a brick:Zone_Air_Dewpoint_Sensor ;
	brick:hasUnits unit:DEG_F .

